<Application x:Class="AprelKonsaltingAPP.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AprelKonsaltingAPP"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="PositiveButton" Color="#A161E5"/>
        <SolidColorBrush x:Key="NegativeButton" Color="#EA5436"/>
        <SolidColorBrush x:Key="OtherButton" Color="#439C06"/>
        <SolidColorBrush x:Key="BoldText" Color="#3A770A"/>
        <SolidColorBrush x:Key="ButtonText" Color="#FFFFFF"/>
        <BitmapImage x:Key="picture" UriSource="/Resources/picture.png"/>
    </Application.Resources>
</Application>
